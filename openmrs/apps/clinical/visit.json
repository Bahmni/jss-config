{
  "general": {
    "title": "General",
    "displayByDefault": true,
    "printing": {
      "title": "Jan Swasthya Sahyog Community Health Centre",
      "header": "Visit",
      "logo": "../images/jss-icon.png"
    },
    "defaultSections": true,
    "sections": {
      "pivotTable": {
        "type": "pivotTable",
        "title": "Pivot Table",
        "config": {
          "groupBy": "encounters",
          "obsConcepts": [
            "Weight",
            "Vitals"
          ],
          "drugConcepts": [],
          "labConcepts": []
        },
        "displayOrder": 0
      }
    }
  },
  "dischargeSummary": {
    "title": "Discharge Summary",
    "printing": {
      "title": "Jan Swasthya Sahyog Community Health Centre",
      "header": "Discharge Summary",
      "logo": "../images/jss-icon.png"
    },
    "sections": {
      "patientInformation": {
        "type": "patientInformation",
        "config": {
          "addressFields": [
            "cityVillage",
            "address2",
            "address3",
            "countyDistrict"
          ],
          "title": "PatientInformation",
          "patientAttributes": []
        },
        "displayOrder": 0
      },
      "admissionDetails": {
        "type": "admissionDetails",
        "config": {
          "showDetailsButton": false,
          "title": "Admission Details"
        },
        "displayOrder": 1
      },
      "atAdmission": {
        "type": "observation",
        "title": "At Admission",
        "config": {
          "conceptNames": [
            "Weight",
            "Height",
            "BMI"
          ],
          "scope": "initial"
        },
        "displayOrder": 2
      },
      "lastRecordedWeight": {
        "type": "observation",
        "title": "Last Recorded Weight",
        "config": {
          "conceptNames": [
            "Weight"
          ],
          "scope": "latest"
        },
        "displayOrder": 3
      },
      "diagnoses": {
        "type": "diagnosis",
        "config": {
          "title": "Diagnoses",
          "showCertainty": false,
          "showOrder": false,
          "showDetailsButton": true
        },
        "displayOrder": 4
      },
      "clinicalNotes": {
        "type": "observation",
        "title": "Clinical Notes",
        "config": {
          "conceptNames": [
            "History and Examination Notes",
            "Discharge Summary, Admission Indication",
            "Hospital Course",
            "Discharge Summary, Surgical Indication",
            "Date of Operation",
            "Operative Procedure",
            "Lab Investigation Notes",
            "Radiology Notes",
            "Other Notes"
          ],
          "scope": "latest",
          "numberOfVisits": "1"
        },
        "displayOrder": 5
      },
      "labInvestigations": {
        "type": "investigationResult",
        "config": {
          "title": "Lab Investigations",
          "showChart": true,
          "showTable": false,
          "showNormalLabResults": true,
          "showCommentsExpanded": true,
          "showAccessionNotes": true,
          "numberOfVisits": 10,
          "initialAccessionCount": 1,
          "latestAccessionCount": 1
        },
        "displayOrder": 6
      },
      "treatments": {
        "type": "treatment",
        "config": {
          "title": "Treatments",
          "showFlowSheet": false,
          "showListView": true,
          "showOtherActive": true,
          "showDetailsButton": false,
          "showRoute": true,
          "showDrugForm": true
        },
        "displayOrder": 7
      },
      "adviceOnDischarge": {
        "type": "observation",
        "title": "Advice on Discharge",
        "config": {
          "conceptNames": [
            "Advice on Discharge",
            "Follow up Date",
            "Discharge Summary, Plan for follow up"
          ],
          "scope": "latest"
        },
        "displayOrder": 8
      }
    }
  },
  "rntcpform": {
    "title": "RNTCP Form",
    "printing": {
      "title": "",
      "header": "",
      "logo": ""
    },
    "sections": {
      "RNTCP Form": {
        "type": "custom",
        "displayOrder": 1,
        "config": {
          "title": "RNTCP Request for Culture and Drug Sensitivity Testing",
          "template": "<rntcp-form></rntcp-form>"
        }
      }
    }
  },
  "clinicalinfotbform": {
    "title": "RMRCT Form",
    "printing": {
      "title": "",
      "header": "",
      "logo": ""
    },
    "sections": {
      "RMRCT Form": {
        "type": "custom",
        "displayOrder": 1,
        "config": {
          "title": "RMRCT Clinical TB Information Form for TB Patient",
          "template": "<clinicalinfo-tb></clinicalinfo-tb>"
        }
      }
    }
  },
  "Death Certificate": {
    "title": "Death Certificate",
    "printing": {
      "title": "",
      "header": "Death Certificate",
      "logo": ""
    },
    "sections": {
      "Death Certificate": {
        "type": "custom",
        "displayOrder": 1,
        "config": {
          "title": "Patient Death Certificate ",
          "template": "<death-certificate></death-certificate>"
        }
      }
    }
  },
  "pplhlthsptgrp": {
    "title": "PD Hinduja Form",
    "printing": {
      "title": "",
      "header": "",
      "logo": "../images/jss-icon.png"
    },
    "sections": {
      "PD Hinduja Form": {
        "type": "custom",
        "displayOrder": 1,
        "config": {
          "title": "Jan Swasthya Sahyog",
          "template": "<pplhlth-sptgrp></pplhlth-sptgrp>"
        }
      }
    }
  }
}




